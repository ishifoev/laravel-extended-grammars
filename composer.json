{
  "name": "ishifoev/laravel-extended-grammars",
  "description": "Extends Laravel's database grammars with advanced SQL helpers like insertOrUpdateUsing().",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Ismoil Shifoev",
      "email": "ismoil.shifoev94@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ishifoev\\ExtendedGrammars\\": "src/"
    }
  },
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "illuminate/database": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^8.0",
    "vimeo/psalm": "^6.0",
    "laravel/pint": "^1.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox",
    "lint": "vendor/bin/pint",
    "analyze": "vendor/bin/psalm"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ishifoev\\ExtendedGrammars\\Providers\\ExtendedGrammarsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
